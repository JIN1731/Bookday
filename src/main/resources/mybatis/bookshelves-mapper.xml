<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Bookshelves">

	<select id="selectRentalListById" resultType="kh.bookday.dto.RentalDTO">
		select * from rental where id = #{id}
	</select>
	
	<select id="selectWishlistListById" resultType="kh.bookday.dto.WishlistDTO">
		select * from wishlist where id = #{id}
	</select>
	
	
	<select id="selectPostListById" resultType="kh.bookday.dto.PostDTO">
		<![CDATA[
		    select * from (select * from post order by p_seq desc) where p_writer_id = #{id} and rownum between 0 and 20
		]]>
	</select>
</mapper>